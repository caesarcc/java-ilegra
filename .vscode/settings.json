{
    "java.checkstyle.configuration": "/google_checks.xml",
    "java.checkstyle.version": "8.18",
    "editor.rulers": [80, 120],
    "editor.insertSpaces": true,
    "editor.tabSize": 2,
    "editor.detectIndentation": false,
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    }      
}